{
    "name": "1143520/flarum_doro",
    "description": "Convert text and unicode emoji into Twemoji with custom image source.",
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Doro\\Twemoji\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Custom Twemoji",
            "category": "feature",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#FECC4D"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}